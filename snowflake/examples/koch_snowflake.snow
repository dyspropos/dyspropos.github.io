system test using svg {

  start {
    forward(200.0)
    turn(240.0)
    forward(200.0)
    turn(240.0)
    forward(200.0)
    path( style(1.0, #000000, #ffffff), identity_pose() )
  }

  rule forward if length > 5.0 {
    let d = length / 3.0
    forward(d)
    turn(60)
    forward(d)
    turn(-120)
    forward(d)
    turn(60)
    forward(d)
  }

}